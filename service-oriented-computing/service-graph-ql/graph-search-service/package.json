{
  "name": "service-graph-ql",
  "version": "1.0.0",
  "description": "GraphQL Service for Smart Tourism",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "seed": "node seed.js",
    "docker:build": "docker build -t smart-tourism-graphql .",
    "docker:run": "docker run -p 4000:4000 --env-file .env.docker smart-tourism-graphql",
